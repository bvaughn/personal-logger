{
  "name": "personal-logger",
  "description": "Webapp for tracking what you eat, how you sleep, and how you feel",
  "author": "Brian Vaughn <brian.david.vaughn@gmail.com>",
  "license": "MIT",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "firebase": "^4.6.2",
    "flow-bin": "^0.60.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "prettier": "^1.9.1",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "react-virtualized": "^9.13.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "flow": "flow",
    "postbuild": "cd build && rm ./static/js/*.js.map && rm ./static/css/*.css.map",
    "precommit": "lint-staged",
    "prettier": "prettier --single-quote --trailing-comma=es5 --write 'src/**/*.{js,jsx,json,css}'",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom"
  }
}
